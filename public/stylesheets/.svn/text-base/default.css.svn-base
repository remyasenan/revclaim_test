.center
{
	text-align: center;
}

.full
{
	width: 95%;
}

.one_third
{
	width: 33%;
}

.two_third
{
	width: 66%;
}

.info
{
	background-color: #e9eef2;
	border: 1px solid #eee;
	padding: 10px
}

.light_info
{
	margin-top: 5px;
	margin-bottom: 5px;
	background-color: #e9eef2;
	border: 1px solid #eee;
         width: 900px;
}

#header
{
	color: #999;
}

#logo_container
{
	padding-top: 5px;
	font-family: Verdana, sans-serif;
	font-size: 160%;
	font-weight: bold;
	color: #30518E;
}

#menu_container ul
{
	list-style: none;
	text-align: right;
	padding-right: 10px;
	border-bottom: 2px solid #5ba86a;
}

#menu_container li
{
	display: inline;
}

#menu_container a:link, #menu_container a:visited
{
	text-decoration: none;
	color: #fff;
	background-color: #5ba86a;
	padding-left: 10px;
	padding-bottom: 3px;
	padding-right: 10px;
	padding-top: 3px;
}

#menu_container a:hover, #menu_container a:active
{
	background-color: #309649;
}

#page_container
{
	padding-top: 10px;
	padding-left: 20px;
	padding-right: 20px;
}

#footer
{
	border-top: 1px solid #ddd;
	text-align: center;
	font-family: sans-serif;
	color: #999;
	margin: 10px;
	padding: 5px;
}

h1
{
	font-size: 120%;
	color: #000000;
}

h2
{
	font-size: 11px;
	color: #990000;
}

h3
{
	font-size: 110%;
	color: #000099;
}

h4
{
	font-size: 105%;
	color: #000099;
}

h7
{
	font-size: smaller;
	color: grey;
}

hr
{
	border: 1px solid #eee;
}

.highlight
{
	background-color: green;
}

.left
{
	text-align: left;
}

.right
{
	text-align: right;
}

.input
{
	background-color: #f9f9f9;
	border: 1px solid #ccc;
	padding: 2px;
}

.input:hover
{
	background-color: #ffffdd;
}

input, textarea
{
	background-color: #f9f9f9;
	border: 1px solid #ccc;
	padding: 2px;
}

input:hover, textarea:hover
{
	background-color: #ffffdd;
}

.button
{
	color: #fff;
	margin: 0;
	background-color: #333;
}

.button:hover
{
	background-color: #600;
}

/* table sortable element */
table.sortable span.sortarrow
{
	color: black;
	text-decoration: none;
}

table.sortable a.sortheader
{
	color: #000000;
	text-decoration: none;
}

/* Legend Styles */
#legend
{
	width: 100%;
	text-align: left;
	margin: 1em 0;
	padding: .5em;
	display: block;
}

#legend dt
{
	background: #fff;
	border: 1px solid #999;
 	float: left;
 	margin: .1em .5em .1em 2em;
 	overflow: hidden;
 	width: .8em; height: .8em;
}

#legend dl, #legend dd
{
 	display: inline;
 	float: left;
 	padding: 0;
 	margin: 0;
 	margin-right: .5em;
}

/* Sort fields */
th a:link, th a:visited
{
	text-decoration: none;
	color: #000033;
}

th a:hover, th a:active
{
	text-decoration: none;
	color: #000066;
}

a img
{
	text-decoration: none;
	border: 0;
}

.tdbold
{
	font-size: 10px;
	font-weight: bold;
	border: 1px solid #000;
	width: 20%;
}

