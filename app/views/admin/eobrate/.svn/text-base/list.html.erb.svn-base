<h1>Listing EOB Rates</h1>

<table>
  <tr>
  <% for column in Eobrate.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  <th>Client</th>
  </tr>
  
<% for eobrate in @eobrates %>
  <tr>
  <% for column in Eobrate.content_columns %>
    <td><%=h eobrate.send(column.name) %></td>
  <% end %>
  <td><%= eobrate.client%></td>
	<!--<td><%= link_to 'Show', :action => 'show', :id => eobrate %></td>-->
    <td><%= link_to 'Edit', :action => 'edit', :id => eobrate %></td>
    <td><%= link_to 'Delete', { :action => 'destroy', :id => eobrate }, :confirm => 'Are you sure?', :post => true %></td>
  </tr>
<% end %>
</table>

<%= link_to 'Previous page', { :page => @eobrate_pages.current.previous } if @eobrate_pages.current.previous %>
<%= link_to 'Next page', { :page => @eobrate_pages.current.next } if @eobrate_pages.current.next %> 

<br />

<%= link_to 'New EOB Rate', :action => 'new' %>
