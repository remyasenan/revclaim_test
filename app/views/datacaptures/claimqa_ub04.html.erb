                if ($(this).value.length == 5) {
                    var url = "/cpt_codes/show?code=" + $(this).value;
                    //"/cpt_codes/show?code=" + $(this).value;
                    new Ajax.Request(url, {
                        method: 'get',
                        onSuccess: function(transport) {
