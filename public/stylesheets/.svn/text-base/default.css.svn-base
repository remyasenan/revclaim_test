table
{
	border: 0px solid #ddd;
	margin-top: 10px;
	margin-bottom: 10px;
}

.table_border
{
	border: 1px solid #000;
	margin-top: 10px;
	margin-bottom: 10px;
	width: 60%;
	text-align: center;
}

th
{
	border: 1px solid #ddd;
	background-color: #eee;
	padding: 1px;
}

.user_box
{
	background-color: #eeeeff;
	padding: 10px;
	float: right;
	border: 1px solid #ccc;
}

td
{
	border: 1px solid #ddd;
	background-color: #fff;
	padding: 1px;
}

.red, #legend .red
{
