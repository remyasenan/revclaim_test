/* CSS Document */
 .imported, #legend .grey
{
	background-color: #cccccc;
}
.certain, #legend .green
{
	background-color: #beff7d;
}
.uncertain, #legend .red
{
	background-color: #ff9797;
}
.blank, #legend .white
{
	background-color: #fff;
}
